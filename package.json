{
    "name": "ui5-explorer",
    "displayName": "UI5 Explorer",
    "description": "UI5 Api Reference Explorer",
    "icon": "media/extension_icon.png",
    "version": "0.2.1",
    "publisher": "michalkozubik",
    "repository": {
        "type": "git",
        "url": "https://github.com/kozubikmichal/vscode-ui5-explorer"
    },
    "bugs": {
        "url": "https://github.com/kozubikmichal/vscode-ui5-explorer/issues",
        "email": "kozubik.michal@gmail.com"
    },
    "homepage": "https://github.com/kozubikmichal/vscode-ui5-explorer",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Programming Languages",
        "Other"
    ],
    "activationEvents": [
        "onView:ui5ApiReference",
        "onCommand:extension.ui5explorer.search"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [{
                "command": "extension.ui5explorer.search",
                "title": "Search",
                "category": "UI5 Explorer",
                "icon": "media/icon-search.svg"
            },
            {
                "command": "extension.ui5explorer.clearCache",
                "title": "Clear Cache",
                "category": "UI5 Explorer"
            }
        ],
        "viewsContainers": {
            "activitybar": [{
                "id": "ui5-explorer",
                "title": "UI5",
                "icon": "media/ui5.png"
            }]
        },
        "views": {
            "ui5-explorer": [{
                "id": "ui5ApiReference",
                "name": "Api Reference"
            }]
        },
        "menus": {
            "view/title": [{
                "command": "extension.ui5explorer.search",
                "when": "view == ui5ApiReference",
                "group": "navigation"
            }]
        },
        "configuration": [{
            "title": "UI5 Explorer",
            "properties": {
                "ui5explorer.framework": {
                    "enum": [
                        "OpenUI5",
                        "SAPUI5"
                    ],
                    "default": "OpenUI5"
                },
                "ui5explorer.version": {
                    "type": "string",
                    "default": ""
                }
            }
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mkdirp": "^0.5.2",
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "@types/rimraf": "^2.0.2",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.18"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "fs": "0.0.1-security",
        "http": "0.0.0",
        "mkdirp": "^0.5.1",
        "path": "^0.12.7",
        "rimraf": "^2.6.2",
        "typescript-ioc": "^1.2.4"
    }
}