{
    "name": "ui5-explorer",
    "displayName": "ui5-explorer",
    "description": "UI5 Api Reference Explorer",
    "version": "0.0.1",
    "publisher": "kozubikmichal",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:ui5ApiReference",
        "onCommand:extension.sayHello"
    ],
    "main": "./out/extension",
    "contributes": {
        "viewsContainers": {
            "activitybar": [{
                "id": "ui5-explorer",
                "title": "UI5 Explorer",
                "icon": "media/ui5.png"
            }]
        },
        "views": {
            "ui5-explorer": [{
                "id": "ui5ApiReference",
                "name": "Api Reference"
            }]
        },
        "commands": [{
            "command": "extension.sayHello",
            "title": "Hello World"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}